if game.PlaceId == 9421227126 then
 
local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "Dank Murderer 2", HidePremium = false, SaveConfig = true, ConfigFolder = "OrionTest"})


local hatchTab = Window:MakeTab({
	Name = "Main",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})


hatchTab:AddToggle({
	Name = "Open Amethys Create",
	Default = false,
	Callback = function(Value)
		local args = {
    [1] = "Amethyst Crate"
}

game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("OpenCrate"):FireServer(unpack(args))
	end    
})


hatchTab:AddToggle({
	Name = "Auto Farm Coins",
	Default = false,
	Callback = function(Value)
		loadstring(game:HttpGet('https://raw.githubusercontent.com/hunqscript/hunqscript/main/TheDankOfMurderer2AutoFarmCoins'))()
})


end
